    "@heroui/theme": "^2.4.20",
    "@heroui/user": "^2.2.20",
    "@hookform/resolvers": "^5.2.1",
    "@tanstack/react-query": "^5.85.5",
    "@types/js-cookie": "^3.0.6",
    "axios": "^1.7.7",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "framer-motion": "^12.23.12",
    "jose": "^6.0.13",
    "js-cookie": "^3.0.5",
    "next": "15.5.0",
    "next-intl": "^4.3.5",
    "next-themes": "^0.4.4",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-hook-form": "^7.53.2",
    "react-icons": "^5.3.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3",
    "@eslint/js": "^9.34.0",
    "@next/bundle-analyzer": "^15.5.0",
    "@next/eslint-plugin-next": "^15.5.0",

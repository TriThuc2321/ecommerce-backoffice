{
  "name": "ecommerce-backoffice",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev --turbopack",
    "dev-domain-https": "HTTPS=true NODE_EXTRA_CA_CERTS=$(mkcert -CAROOT)/rootCA.pem next dev --turbopack -p 3400 & local-ssl-proxy --key backoffice.myenglish.com-key.pem --cert backoffice.myenglish.com.pem --source 443 --target 3400",
    "dev-https": "next dev --experimental-https",
    "eslint:check": "eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "eslint:fix": "npm run eslint:check -- --fix",
    "lint": "npm run eslint:fix && npm run prettier:write",
    "lint-staged": "npx lint-staged",
    "precommit": "lint-staged",
    "prepare": "husky",
    "prettier:check": "prettier --check \"./**/*.{ts,tsx,js,json,*rc}\"",
    "prettier:write": "prettier --write \"./**/*.{ts,tsx,js,jsx,json,*rc}\"",
    "start": "next start",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
